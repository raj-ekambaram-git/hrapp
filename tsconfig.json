{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/index.js", "store/store.js", "store/authSlice.js", "store/index.ts", "pages/timesheet/add.jsx", "components/styles/inputLeftElementTheme.jsx", "pages/account/dashboard.jsx", "pages/register.jsx", "pages/_app.jsx", "components/configuration/user/managePaymentAccounts.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
